<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping">

    <class name="ResponseObject" table="results">
        <id name="jsessionid" column="jsessionid"/>
        <property name="id" column="id"/>
        <property name="x_value" column="x_value"/>
        <property name="y_value" column="y_value"/>
        <property name="r_value" column="r_value"/>
        <property name="script_time" column="script_time"/>
        <property name="current_time" column="current_time"/>
        <property name="hit_result" column="hit_result"/>
    </class>
</hibernate-mapping>